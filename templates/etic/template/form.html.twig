{% extends 'baseEtic.html.twig' %}

{% block body %}


    <div class="row bg-title">
        <!-- .page title -->
        <div class="col-sm-4 col-xs-12">
            {% if title is defined %}
                <h4 class="page-title">{{ title }}</h4>
            {% endif %}
        </div>
        <!-- /.page title -->
        <!-- .breadcrumb -->
        <div class="col-sm-4 col-xs-12">
            <ol class="breadcrumb">
                <li><a href="#">Anasayfa</a></li>
                {% if title is defined %}
                    <li class="active">{{ title }}</li>
                {% endif %}
            </ol>
        </div>
        <!-- /.breadcrumb -->
        <div class="col-sm-4 col-xs-12 text-right"> 
        </div>
    </div>

    <div class="row">
        {% if image is defined %}
            <div class="col-md-4">
                <div class="white-box">
                    <img src="{{ image }}" width="100%" >
                </div>
            </div>
        {% endif %}
        <div class="col-md-12">
            <div class="white-box">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
{% endblock %}